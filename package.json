{
  "name": "coolifyjs",
  "version": "0.1.0",
  "description": "A TypeScript SDK for the Coolify API",
  "type": "module",
  "main": "./dist/coolifyjs.cjs",
  "module": "./dist/coolifyjs.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/coolifyjs.js",
      "require": "./dist/coolifyjs.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run test:run && npm run build",
    "release": "npm run test:run && npm run build && npm publish",
    "release:dry": "npm run test:run && npm run build && npm publish --dry-run",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "keywords": [
    "coolify",
    "coolifyjs",
    "api",
    "sdk",
    "typescript",
    "devops",
    "self-hosted",
    "paas",
    "deployment",
    "docker"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neelansh15/coolifyjs.git"
  },
  "bugs": {
    "url": "https://github.com/neelansh15/coolifyjs/issues"
  },
  "homepage": "https://github.com/neelansh15/coolifyjs#readme",
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
